        chronom_cli_delete_chronom_user_iam_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  action="delete chronom user-iam"

  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in

      --name | -n)

        if [[ -n ${2+x} ]]; then

          args['--name']="$2"
          shift
          shift
        else
          printf "%s\n" "--name requires an argument: --name, -n NAME" >&2
          exit 1
        fi
        ;;

